package capaNegocio;

import java.util.ArrayList;


public class Trabajador extends Persona{
 private String profesion;
 private String puesto;
 private String experiencia;  

public ArrayList <Cliente> verifica = new ArrayList();
public void verificarcliente(Cliente Cliente){
verifica.add(Cliente);
}
public ArrayList <Pagos> realizanlos = new ArrayList();
public void realizalos(Pagos Pagos){
realizanlos.add(Pagos);
}
public String getProfesion(){
return profesion; 
}
public void setProfesion(String profesion){
    this.profesion = profesion;
}
public String getPuesto(){
return puesto; 
}
public void setPuesto(String puesto){
    this.puesto = puesto;
}
public String getExperiencia(){
return experiencia; 
}
public void setExperiencia(String experiencia){
    this.experiencia = experiencia;
}
}
